POSTGRES_MASTER_URL=postgresql+asyncpg://postgres:postgres@postgres:5432/master
# If a tenant database is missing, scripts will connect to "postgres" (falling
# back to "template1") to create it before applying schema.
POSTGRES_TENANT_DSN_TEMPLATE=postgresql+asyncpg://postgres:postgres@postgres:5432/tenant_{tenant_id}
JWT_SECRET=change_me
REDIS_URL=redis://redis:6379/0
ALLOWED_ORIGINS=https://example.com
ENABLE_HSTS=1
BODY_MAX_KB=128
ADMIN_API_ENABLED=false
SSE_KEEPALIVE_INTERVAL=15
MAX_CONN_PER_IP=20
QUEUE_MAX=100
EXPORT_MAX_ROWS=10000
ENV=prod
HEARTBEAT_TIMEOUT_SEC=30
TENANT_ANALYTICS_ENABLED=1
ANALYTICS_TENANTS=tenant_a,tenant_b
FLAG_SIMPLE_MODIFIERS=1
FLAG_WA_ENABLED=1
AB_TESTS_ENABLED=0
